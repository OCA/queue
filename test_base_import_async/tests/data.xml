<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- hack to set an xmlid on some demo data that have non in v9 -->
    <record id="testjournal" model="ir.model.data">
        <field name="res_id" search="[('code','=','MISC')]" model="account.journal"/>
        <field name="model">ir.model.fields</field>
        <field name="module">test_base_import_async</field>
        <field name="name">testjournal_xmlid</field>
        <field name="noupdate">True</field>
    </record>
    <record id="a_recv" model="ir.model.data">
        <field name="res_id" search="[('code','=','101200')]" model="account.account"/>
        <field name="model">ir.model.fields</field>
        <field name="module">test_base_import_async</field>
        <field name="name">a_recv_xmlid</field>
        <field name="noupdate">True</field>
    </record>
    <record id="a_sale" model="ir.model.data">
        <field name="res_id" search="[('code','=','200000')]" model="account.account"/>
        <field name="model">ir.model.fields</field>
        <field name="module">test_base_import_async</field>
        <field name="name">a_sale_xmlid</field>
        <field name="noupdate">True</field>
    </record>
</odoo>
